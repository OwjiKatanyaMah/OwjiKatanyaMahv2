 AddPlayerSalary(playerid, "Hauling Trucker", 3600);
				pData[playerid][pJobTime] += 900;
                DialogHauling[0] = false;
                DialogSaya[playerid][0] = false;
                DestroyVehicle(GetVehicleTrailer(GetPlayerVehicleID(playerid)));
                Truck(playerid, "{00ff00}$1.200 have been issued to your {FFFF00}/salary");
                return 1;
