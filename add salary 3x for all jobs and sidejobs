                Truck(playerid, "{00ff00}$1.400 {ffffff}have been issued to your {FFFF00}/salary");
                return 1;
			}
		}
		else if(SedangHauling[playerid] == 19)
		{
			DisablePlayerRaceCheckpoint(playerid);
     		Truck(playerid, "{ffffff}Attach the trailer to your vehicle to order");
